{
    "name": "syril/wc",
    "description": "Want & can portal",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Konstantin Kotov",
            "email": "kotov@nsod.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0",
        "yiisoft/yii2-debug": "^2.1",
        "yiisoft/yii2-gii": "^2.2",
        "yiisoft/yii2-bootstrap5": "*",
        "kartik-v/yii2-widget-datepicker": "^1.4",
        "mihaildev/yii2-ckeditor": "^1.0",
        "yiisoft/yii2-swiftmailer": "^2.1",
        "kartik-v/yii2-widget-activeform": "^1.6",
        "kartik-v/yii2-widget-switchinput": "^1.3",
        "kartik-v/yii2-grid": "^3.5",
        "kartik-v/yii2-widget-select2": "^2.2",
        "kartik-v/yii2-bootstrap5-dropdown": "^1.0"        
    },
    "autoload": {
        "psr-4": {
            "app\\": "app"
        }
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://asset-packagist.org"
      }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer" : true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    }
}
